{
  "tasks": {
    "dev": "deno run --watch src/helpers/sample_usage.ts",
    "test": "deno test",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "update-code3": "deno run --allow-read --allow-write src/helpers/update_code3.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "name": "@algoflows/flagmoji",
  "version": "0.1.1",
  "description": "A comprehensive library for working with country flag emojis, supporting ISO 3166-1 codes and country information",
  "runtime": {
    "deno": true,
    "node": true,
    "workerd": true,
    "bun": true
  },
  "license": "MIT",
  "exports": "./mod.ts",
  "author": "Sean Knowles <s@seanknowles.dev>",
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 80,
    "semiColons": true,
    "singleQuote": false
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "publish": {
    "include": [
      "src/data/data.json",
      "src/types/",
      "src/main.ts",
      "mod.ts",
      "README.md",
      "LICENSE",
      "deno.json",
      "CHANGELOG.md",
      "CONTRIBUTING.md"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algoflows/open-source.git",
    "directory": "packages/flagmoji"
  },
  "bugs": {
    "url": "https://github.com/algoflows/open-source/issues"
  },
  "homepage": "https://github.com/algoflows/open-source/tree/main/packages/flagmoji#readme"
}